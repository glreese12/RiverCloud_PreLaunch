
 
#border-radius-combo {
	-webkit-border-radius-left:  5px 80px 5px;
	-moz-border-radius:  5px 80px 5px;
	border-radius:  5px 80px 5px
}


.carousel { z-index: -99; } /* keeps this behind all content */
.carousel .item {
    position: fixed;
    width: 100%; height: 100%;
    -webkit-transition: opacity 1s;
    -moz-transition: opacity 1s;
    -ms-transition: opacity 1s;
    -o-transition: opacity 1s;
    transition: opacity 1s;
 
}
.carousel .one {
    background: url(<%= asset_path "summer.jpg" %>);
    background-size: cover; 
    -moz-background-size: cover;
}
.carousel .two {
    background: url(<%= asset_path "fall.jpg" %>);
    background-size: cover;
    -moz-background-size: cover;
}
.carousel .three {
    background: url(<%= asset_path "winter.jpg" %>);
    background-size: cover;
    -moz-background-size: cover;
}
.carousel .active.left {
    left:0;
    opacity:0;
    z-index:2;
}

